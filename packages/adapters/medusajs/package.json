{
  "name": "@paykit-sdk/medusajs",
  "version": "1.0.0",
  "description": "MedusaJS Integrations for Paykit",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "rm -rf dist && npm run build"
  },
  "keywords": [
    "medusajs",
    "paykit",
    "typescript",
    "payment"
  ],
  "author": "Emmanuel Odii",
  "license": "ISC",
  "packageManager": "pnpm@10.13.1",
  "peerDependencies": {
    "@medusajs/framework": ">=2.10.3",
    "@paykit-sdk/core": ">=1.1.9"
  },
  "devDependencies": {
    "@medusajs/framework": "^2.10.3",
    "@paykit-sdk/core": "workspace:*",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "zod": "^3.25.76"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/usepaykit/paykit-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/usepaykit/paykit-sdk/issues"
  }
}
