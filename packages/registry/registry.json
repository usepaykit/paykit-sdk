        {
          "path": "packages/registry/stripe/nextjs/hooks/checkout-form.tsx",
          "type": "registry:component",
          "target": "app/components/checkout-form.tsx"
        }
      ],
      "docs": "Add the PaykitWrapper to your Next.js layout file to wrap your app in the PaykitProvider.",
      "meta": {
        "backend": "nextjs",
        "provider": "stripe",
        "orm": "none",
        "database": "none",
        "auth": "none"
      }
    },
    {
      "name": "stripe-nextjs-prisma",
      "type": "registry:block",
      "description": "Stripe payment integration for Next.js App Router with Prisma ORM",
      "dependencies": ["@prisma/client"],
      "devDependencies": ["prisma"],
